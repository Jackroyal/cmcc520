{
    "name":"cmcc520",
    "version":"2.0",
    "description":"powered by 搁浅St",
    "background": {
        "scripts": ["bg.js","js/function.js","js/lib.js"],
        "persistent": false
    },
    "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/function.js","js/lib.js"]
    }
    ],
    "page_action":{
        "default_icon":"icon-48.png",
        "default_title":"cmcc520"
    },
    "permissions" : [
        "tabs", "http://*/*"
    ],
    "icons" : {
        "48" : "icon-48.png",
        "128" : "icon-128.png"
    },
    "commands": {
        "logout_cmcc": {
            "description": "退出CMCC登录",
            "suggested_key": {
                "default": "Alt+X"
            }
        }
    },
    "manifest_version":2

}
